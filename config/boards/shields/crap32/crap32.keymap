        tog_NAVI_OUT: tog_NAVI_OUT {
            label = "ZM_to_NAVI_OUT";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            wait-ms = <0>;
            tap-ms = <0>;
            bindings
                = <&rgb_ug RGB_COLOR_HSB(30,95,2)>
