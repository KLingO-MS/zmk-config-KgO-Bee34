#define _SYM_SK     3
#define _NAVI       4
#define _FKEYS      5
#define _NUMPAD     6

&sk {
    quick-release;
};

/ {
    macros {
        to_BASE_EN: to_BASE_EN {
            label = "ZM_to_BASE_EN";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            wait-ms = <0>;
            tap-ms = <0>;
            bindings
                = <&rgb_ug RGB_COLOR_HSB(30,95,2)>
                , <&to _BASE_EN>
                , <&kp LGUI(SPACE)>
                ;
        };

        to_BASE_SK: to_BASE_SK {
            label = "ZM_to_BASE";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            wait-ms = <0>;
            tap-ms = <0>;
            bindings
                = <&rgb_ug RGB_COLOR_HSB(30,95,2)>
                , <&to _BASE_SK>
                , <&kp LGUI(SPACE)>
                ;
        };

        tog_NAVI_IN: tog_NAVI_IN {
